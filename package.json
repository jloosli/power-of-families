{
  "name": "power-of-families-theme",
  "version": "3.0.0",
  "description": "Custom wordpress theme for Power of Families",
  "main": "scripts.js",
  "author": "Jared Loosli",
  "license": "UNLICENSED",
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "version-bump": "node ./bin/bump-version",
    "deploy": "npm run deploy:theme",
    "deploy:theme": "npm run build && rsync -avzhn --exclude=./power-of-families-theme/assets ./power-of-families-theme/ pof:html/wp-content/themes/power-of-families"
  },
  "devDependencies": {
    "@wordpress/babel-preset-default": "^7.38.0",
    "@wordpress/prettier-config": "^3.11.0",
    "@wordpress/scripts": "^27.5.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.3",
    "webpack-remove-empty-scripts": "^1.0.4",
    "wp-types": "^3.64.0"
  }
}
